<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <!-- generic -->
    <suppress files=".*TestData\.java" checks=".*"/>
    <suppress files=".*Test\.java" checks=".*"/>
    <suppress files=".*Defs\.java" checks=".*"/>
    <suppress files=".*Stub\.java" checks=".*"/>
    <suppress files=".*[\\/]generated[\\/].*" checks=".*"/>
    <suppress files=".*[\\/]generated-sources[\\/].*" checks=".*"/>

    <!-- Alphabetically sorted -->
    <suppress files="FmpHttpClientImpl.java" checks="AvoidInlineConditionals"/> <!-- reason: getJsonList() -->
    <suppress files="FmpRatioTtm.java" checks="AbbreviationAsWordInName"/> <!-- netIncomePerEBTTTM -->
</suppressions>